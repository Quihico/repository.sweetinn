<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
	<name>Last played episodes</name>
	<match>all</match>
	<rule field="playcount" operator="greaterthan">
		<value>0</value>
	</rule>
	<order direction="descending">lastplayed</order>
	<limit>20</limit>
</smartplaylist>
