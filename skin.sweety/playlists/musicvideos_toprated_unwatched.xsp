<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="musicvideos">
	<name>Top rated and unwatched music videos</name>
	<match>all</match>
	<rule field="playcount" operator="lessthan">
		<value>1</value>
	</rule>
	<rule field="rating" operator="greaterthan">
		<value>0</value>
	</rule>
	<limit>100</limit>
	<order direction="descending">rating</order>
</smartplaylist>