<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
	<name>Friendly, unwatched episodes</name>
	<match>all</match>
	<rule field="playcount" operator="lessthan">
		<value>1</value>
	</rule>
	<rule field="genre" operator="doesnotcontain">
		<value>Horror</value>
		<value>Thriller</value>
	</rule>
	<order direction="ascending">dateadded</order>
	<limit>20</limit>
</smartplaylist>
