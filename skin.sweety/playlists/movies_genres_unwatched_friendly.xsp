<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
	<name>Friendly, unwatched movie genres</name>
	<match>all</match>
	<rule field="playcount" operator="lessthan">
		<value>1</value>
	</rule>
	<rule field="genre" operator="doesnotcontain">
		<value>Horror</value>
		<value>Thriller</value>
	</rule>
	<group>genres</group>
</smartplaylist>
